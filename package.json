{
  "name": "enchantments-for-todoist",
  "version": "3.1",
  "description": "Enchantments for Todoist This extension is going to level up your productivity, by providing some extra features for https://todoist.com/ website",
  "repository": "https://github.com/ntorbinskiy/Enchantments-for-Todoist/",
  "author": "Nikita Torbinskiy",
  "private": true,
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js && npm-build-zip --source=./dist/ --name=build --name_only=true",
    "clean": "rimraf dist && rimraf build.zip",
    "lint": "npx eslint .",
    "test": "jest ."
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "copy-webpack-plugin": "^8.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-jest": "^27.6.0",
    "file-loader": "^6.2.0",
    "npm-build-zip": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.18",
    "typescript": "^5.2.2",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "jest": "^29.7.0",
    "path": "^0.12.7",
    "puppeteer": "^21.5.1",
    "ts-jest": "^29.1.1"
  }
}
